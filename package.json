{
  "name": "nats-memory-server",
  "version": "2.0.15",
  "description": "Node.js package for an in-memory NATS server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "postinstall": "node ./dist/scripts/download-and-build.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "nats",
    "memory",
    "server",
    "NATS",
    "development",
    "natsmem",
    "tests",
    "mock",
    "stub",
    "nats-prebuilt",
    "memory-server"
  ],
  "lint-staged": {
    "src/**/*.{js,ts,json}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "author": "Llirik1337",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Llirik1337/nats-memory-server.git"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.4",
    "@types/node": "^18.16.18",
    "nats": "^2.15.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "lint-staged": "^13.2.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.5"
  },
  "dependencies": {
    "decompress": "^4.2.1"
  },
  "bugs": {
    "url": "https://github.com/Llirik1337/nats-memory-server/issues"
  },
  "homepage": "https://github.com/Llirik1337/nats-memory-server#readme"
}
